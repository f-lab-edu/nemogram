{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb/hooks",
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/jsx-props-no-spreading": [
      "error",
      {
        "exceptions": ["Component"]
      }
    ],
    "react/react-in-jsx-scope": "off"
  }
}
